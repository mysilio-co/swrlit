{
  "name": "swrlit",
  "version": "1.0.0-beta.8",
  "author": "Travis Vachon",
  "description": "swirl the web in your glass",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/itme/swrlit/issues"
  },
  "homepage": "https://swrlit.me/",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "size-limit": [
    {
      "path": "dist/index.cjs.js",
      "limit": "15 KB"
    },
    {
      "path": "dist/index.js",
      "limit": "210 KB"
    }
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "test": "jest",
    "ts-types": "tsc --emitDeclarationOnly --outDir dist",
    "build": "rimraf dist && npm run ts-types && node ./esbuild.js ",
    "size": "size-limit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mysilio-co/swrlit.git"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "keywords": [
    "solid",
    "http",
    "swr",
    "react"
  ],
  "peerDependencies": {
    "react": ">=16"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/jest": "^28.1.4",
    "@types/react": "^18.0.14",
    "esbuild": "^0.14.48",
    "esbuild-node-externals": "^1.4.1",
    "jest": "^28.1.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.5",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@inrupt/solid-client": "^1.23.1",
    "@inrupt/solid-client-authn-browser": "^1.12.1",
    "@inrupt/vocab-common-rdf": "^1.0.3",
    "@inrupt/vocab-solid-common": "^0.7.5",
    "dequal": "^2.0.2",
    "swr": "^1.0.1",
    "url-parse": "^1.4.7"
  }
}
